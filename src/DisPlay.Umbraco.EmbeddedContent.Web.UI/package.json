  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "lint:js": "eslint .",
    "lint:style": "stylelint src/**/*.css",
    "lint": "yarn run flow && yarn run lint:js && yarn run lint:style",
    "fix:js": "eslint . --fix",
    "fix:style": "stylelint src/**/*.css --fix",
