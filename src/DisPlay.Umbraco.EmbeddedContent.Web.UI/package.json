    "clean": "rimraf build",
    "start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env development --progress\"",
    "build": "yarn run clean && webpack --env production --progress",
    "stats": "webpack --env production --profile --json > stats.json"
