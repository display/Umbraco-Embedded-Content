{
  "name": "umbraco-embedded-content",
  "version": "0.1.0",
  "description": "DIS/PLAY Umbraco Embedded Content Property Editor",
  "repository": "",
  "author": "Rasmus John Pedersen <mail@rjp.dk> (http://www.rasmusjp.dk)",
  "license": "MIT",
  "pluginName": "EmbeddedContent",
  "proxy": "http://localhost:49392/",
  "externals": {
    "angular": "angular",
    "jquery": "jQuery",
    "underscore": "underscore"
  },
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "lint:js": "eslint .",
    "lint:style": "stylelint app/**/*.css",
    "lint": "npm run lint:js && npm run lint:style",
    "fix:js": "eslint . --fix",
    "fix:style": "stylelint app/**/*.css --fix",
    "fix": "npm run fix:js && npm run fix:style",
    "clean": "rimraf dist",
    "start": "nodemon --watch webpack.config.babel.js --exec \"webpack-dev-server --config webpack.config.babel.js --env development --progress\"",
    "build": "npm run clean && webpack --config webpack.config.babel.js --mode production --progress",
    "stats": "webpack --config webpack.config.babel.js --mode production --json --profile > stats.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.2",
    "@babel/core": "^7.0.0-rc.2",
    "@babel/node": "^7.0.0-rc.2",
    "@babel/polyfill": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/register": "^7.0.0-rc.2",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^0.4.2",
    "nodemon": "^1.18.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-smart-import": "^0.7.6",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
